{
    "mainAddress": "localhost:27017",
    "oplogFile": "oplog.timestamp",
    "noDump": false,
    "batchSize": -1,
    "uniqueKey": "_id",
    "verbose": 0,

    "logFile": "mongo-connector.log",
    "syslog": {
	"enabled": false,
	"host": "localhost:514"
    },

    "__comment__": "To modify the settings below, remove the preceding '__'",

    "__adminUsername": "username",
    "__password": "password",
    "__passwordFile": "passwordFile",

    "__fields": ["field1", "field2", "field3"],
    "__namespaceSet": ["test.source1", "test.source2"],
    "__destMapping": {"test.source1": "test.dest1", "test.source2": "test.dest2"},

    "docManagers": [
	{
	    "docManager": "mongo_connector/doc_managers/elastic_doc_manager.py",
	    "targetURL": "localhost:9200",
	    "uniqueKey": "_id",
	    "autoCommitInterval": null
	}
    ]
}
